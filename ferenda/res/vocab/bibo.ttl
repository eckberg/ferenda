:CourtReporter a owl:Class ;
    rdfs:label "Court Reporter"@en ;
    rdfs:comment "A collection of legal cases."@en ;
    rdfs:isDefinedBy "http://purl.org/ontology/bibo/"^^xsd:anyURI ;
    rdfs:subClassOf [ a owl:Restriction ;
